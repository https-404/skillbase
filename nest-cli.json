{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/api-gateway/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/api-gateway/tsconfig.app.json"
  },
  "projects": {
    "api-gateway": {
      "type": "application",
      "root": "apps/api-gateway",
      "entryFile": "main",
      "sourceRoot": "apps/api-gateway/src",
      "compilerOptions": {
        "tsConfigPath": "apps/api-gateway/tsconfig.app.json"
      }
    },
    "course-service": {
      "type": "application",
      "root": "apps/course-service",
      "entryFile": "main",
      "sourceRoot": "apps/course-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/course-service/tsconfig.app.json"
      }
    },
    "identity-service": {
      "type": "application",
      "root": "apps/identity-service",
      "entryFile": "main",
      "sourceRoot": "apps/identity-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/identity-service/tsconfig.app.json"
      }
    },
    "indexer-service": {
      "type": "application",
      "root": "apps/indexer-service",
      "entryFile": "main",
      "sourceRoot": "apps/indexer-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/indexer-service/tsconfig.app.json"
      }
    },
    "media-service": {
      "type": "application",
      "root": "apps/media-service",
      "entryFile": "main",
      "sourceRoot": "apps/media-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/media-service/tsconfig.app.json"
      }
    },
    "progress-service": {
      "type": "application",
      "root": "apps/progress-service",
      "entryFile": "main",
      "sourceRoot": "apps/progress-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/progress-service/tsconfig.app.json"
      }
    },
    "reviews-service": {
      "type": "application",
      "root": "apps/reviews-service",
      "entryFile": "main",
      "sourceRoot": "apps/reviews-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/reviews-service/tsconfig.app.json"
      }
    },
    "shared": {
      "type": "library",
      "root": "libs/shared",
      "entryFile": "index",
      "sourceRoot": "libs/shared/src",
      "compilerOptions": {
        "tsConfigPath": "libs/shared/tsconfig.lib.json"
      }
    }
  },
  "monorepo": true
}