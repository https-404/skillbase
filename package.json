  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start api-gateway",
    "start:dev": "nest start api-gateway --watch",
    "start:debug": "nest start api-gateway --debug --watch",
    "start:prod": "node dist/apps/api-gateway/main",
    "start:api-gateway": "nest start api-gateway --watch",
    "start:identity-service": "nest start identity-service --watch",
    "start:course-service": "nest start course-service --watch",
    "start:media-service": "nest start media-service --watch",
    "start:progress-service": "nest start progress-service --watch",
    "start:reviews-service": "nest start reviews-service --watch",
    "start:indexer-service": "nest start indexer-service --watch",
    "start:all": "concurrently -n \"api-gateway,identity-service,course-service,media-service,progress-service,reviews-service,indexer-service\" -c \"blue,green,yellow,magenta,cyan,red,white\" \"pnpm run start:api-gateway\" \"pnpm run start:identity-service\" \"pnpm run start:course-service\" \"pnpm run start:media-service\" \"pnpm run start:progress-service\" \"pnpm run start:reviews-service\" \"pnpm run start:indexer-service\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "concurrently -n \"api-gateway,identity-service,course-service,media-service,progress-service,reviews-service,indexer-service\" -c \"blue,green,yellow,magenta,cyan,red,white\" \"jest --config ./apps/api-gateway/test/jest-e2e.json\" \"jest --config ./apps/identity-service/test/jest-e2e.json\" \"jest --config ./apps/course-service/test/jest-e2e.json\" \"jest --config ./apps/media-service/test/jest-e2e.json\" \"jest --config ./apps/progress-service/test/jest-e2e.json\" \"jest --config ./apps/reviews-service/test/jest-e2e.json\" \"jest --config ./apps/indexer-service/test/jest-e2e.json\"",
    "test:e2e:api-gateway": "jest --config ./apps/api-gateway/test/jest-e2e.json",
    "test:e2e:identity-service": "jest --config ./apps/identity-service/test/jest-e2e.json",
    "test:e2e:course-service": "jest --config ./apps/course-service/test/jest-e2e.json",
    "test:e2e:media-service": "jest --config ./apps/media-service/test/jest-e2e.json",
    "test:e2e:progress-service": "jest --config ./apps/progress-service/test/jest-e2e.json",
    "test:e2e:reviews-service": "jest --config ./apps/reviews-service/test/jest-e2e.json",
    "test:e2e:indexer-service": "jest --config ./apps/indexer-service/test/jest-e2e.json"
  },
